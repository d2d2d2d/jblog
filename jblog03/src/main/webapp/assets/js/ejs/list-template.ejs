
<%
	for(var i = 0; i < data.length; i++) {
	var vo = data[i];
%>
	<tr>
		<td><%=vo.no%></td>
		<td><%=vo.name%></td>
		<td><%=vo.postCount%></td>
		<td><%=vo.description%></td>
		<% 
			if(vo.postCount == 0) {
		%> 
		<td><a href="<%=pageContext%>/<%=vo.id%>/admin/category/delete/<%=vo.no%>" data-no = '<%=vo.no%>'>
		<img src="<%=pageContext%>/assets/images/delete.jpg"></a></td>
		<%
		} else {
		%>
		<td></td>
		<%
		}
		%>
	</tr>
<%
	}
%>